    {
        "id": "bc807acc.c91fa8",
        "type": "ui_template",
        "z": "87555f5c.75aab",
        "group": "3f08adad.913ada",
        "name": "Classfications",
        "order": 0,
        "width": "7",
        "height": "6",
        "format": "<div>\n    <p style=\"font-size: large; font-weight: bold; color: white;\">Skin temperature classification</p>\n\n    <table style=\"width:100%\">\n      <tr>\n        <td>Rigid muscles, Lethal condition:</td>\n        <td>15 - 19°C</td>\n      </tr>\n      <tr>\n        <td>Possibly coma, Poor reflexes:</td>\n        <td>19 - 27°C</td>\n      </tr>\n      <tr>\n        <td>Stuport, Dozy, Unable to walk:</td>\n        <td>27 - 30°C</td>\n      </tr>\n      <tr>\n        <td>Shivering:</td>\n        <td>30 - 32</td>\n      </tr>\n      <tr>\n        <td>Healthy conditon:</td>\n        <td>32 - 34°C</td>\n      </tr>\n      <tr>\n        <td>Fevery:</td>\n        <td>34 - 36°C</td>\n      </tr>\n      <tr>\n        <td>Lethal condition:</td>\n        <td>36 - 43°C</td>\n      </tr>\n    </table>\n</div>\n<div>\n    <p style=\"font-size: large; font-weight: bold; color: white;\">Heartrate classification</p>\n    \n    <table style=\"width:100%\">\n      <tr>\n        <td>Hypothermic:</td>\n        <td>20 - 40 bpm</td>\n      </tr>\n      <tr>\n        <td>Low repulse:</td>\n        <td>40 - 60 bpm</td>\n      </tr>\n      <tr>\n        <td>Healthy condition:</td>\n        <td>60 - 100 bpm</td>\n      </tr>\n      <tr>\n        <td>Anxiety, Fear, Fever:</td>\n        <td>100 - 150 bpm</td>\n      </tr>\n      <tr>\n        <td>Heavy heart racing:</td>\n        <td>150 - 200 bpm</td>\n      </tr>\n      <tr>\n        <td>Lethal condition:</td>\n        <td>200 - 300 bpm</td>\n      </tr>\n    </table>\n</div>\n<div>\n    <p style=\"font-size: large; font-weight: bold; color: white;\">Respiration rate classification</p>\n    \n    <table style=\"width:100%\">\n      <tr>\n        <td>Non-existent:</td>\n        <td>0 breaths/min</td>\n      </tr>\n      <tr>\n        <td>Abnormally slow:</td>\n        <td>5 - 10 breaths/min</td>\n      </tr>\n      <tr>\n        <td>Healthy condition:</td>\n        <td>12 - 18 breaths/min</td>\n      </tr>\n      <tr>\n        <td>Abnormally fast:</td>\n        <td>20 - 30 breaths/min</td>\n      </tr>\n    </table>\n</div>",
